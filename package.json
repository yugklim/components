    "@storybook/addon-notes": "^3.2.0",
    "@storybook/addon-options": "^3.2.4",
    "@storybook/react": "^3.2.5",
    "@storybook/storybook-deployer": "^2.0.0",
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
